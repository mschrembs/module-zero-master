<div>
    <div class="modal-header">
        <h3 class="modal-title">@L("TenantDelete")</h3>
    </div>
    <div class="modal-body">
        <form class="form-horizontal">
            <fieldset>
                <div class="form-group">
                    <div class="col-lg-10">
                        <span>@L("TenantDeleteQuestion")</span>
                    </div>
                </div>
                <input type="hidden" id="TenantId" ng-model="vm.tenant.id" />
            </fieldset>
        </form>
    </div>
    <div class="modal-footer">
        <button class="btn btn-warning" ng-click="vm.cancel()">Cancel</button>
        <button class="btn btn-primary" ng-click="vm.delete()">@L("TenantDelete")</button>
    </div>
</div>